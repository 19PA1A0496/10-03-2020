      "source": [
        "df['Gender'] = df['Gender'].map({'M':0, 'F':1})\r\n",
        "df['Section'] = df['Section'].map({'A':0,'B':1})\r\n",
        "df['registered_for_ Placement_Training']=df['registered_for_ Placement_Training'].map({'NO':0,'yes':1})\r\n",
